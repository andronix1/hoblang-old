extern fun putchar(c: u8): void

fun works(): void { putchar('w'); putchar('o'); putchar('r'); putchar('k'); putchar('s'); }
fun so_bad(): void {  putchar('s'); putchar('o'); putchar('b'); putchar('a'); putchar('d'); }

fun sum(a: i32, b: i32): i32 { return a + b; }

fun main(): void {
	var a = 3;
	var b = 4;
	if sum(a, b) == a + b {
		works();
	} else {
		so_bad();
	}
	putchar('\n');
}
