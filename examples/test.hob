module main;

extern(putchar) fun put_char(c: u8): void
extern(getchar) fun get_char(): u8
extern(fflush) fun fflush(stream: u32): void

fun main(): void {
	var a = get_char() - '0';
	get_char();
	var b = get_char() - '0';
	if a + b < 10 {
		put_char('0' + a + b);
	} else {
		put_char('1');
		put_char('0' - 10 + a + b);
	}
	put_char('\n');
}
