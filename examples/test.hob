module main;

extern(putchar) fun put_char(c: u8): void

fun works(): void { put_char('w'); put_char('o'); put_char('r'); put_char('k'); put_char('s'); }
fun so_bad(): void { put_char('s'); put_char('o'); put_char('b'); put_char('a'); put_char('d'); }

fun sum(a: i32, b: i32): i32 { return a + b; }

fun main(): void {
	var a: i32 = 3;
	var b: i32 = 4;
	if sum(a, b) == a + b {
		works();
	} else {
		so_bad();
	}
	put_char('\n');
}
