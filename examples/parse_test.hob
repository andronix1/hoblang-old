type MyStruct2 = struct {
    a: i32,
    b: i32
};

type MyStruct = struct {
    a: i32,
    b: i32,
    c: MyStruct2
};

fun test(b: i32) -> MyStruct {
    var a: MyStruct;
    a.c.b = b;
    a.a = 123;
    return a;
}

fun main() -> i32 {
    var ms: MyStruct;
	var testf: fun (i32) -> MyStruct = test;
    ms.a = 0;
    var a: i32 = 0;
    a = 123;
    ms.a = a;
    var ms2 = test(1);
    a = ms2.b;
    return testf(99).c.b;
}
